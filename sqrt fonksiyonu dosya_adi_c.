#include <stdio.h>
#include <math.h> // sqrt() fonksiyonunu kullanabilmek için

int main(){
    
    double sayi, karekok;
    
    //Kullanıcıdan sayı değeri al
    printf("Karekoku hesaplanacak sayiyi giriniz:");
    scanf("%lf", &sayi);
    
    // sayinin karekokunu hesapla
    karekok= sqrt(sayi);
    
    // sonucu yazdır
    printf("%.2lf sayının karekoku = %.2lf", sayi, karekok);
    
    return 0;
}
